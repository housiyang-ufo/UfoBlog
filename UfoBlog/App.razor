<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="@routeData" DefaultLayout="@typeof(SiderNav)">
            <NotAuthorized>
                <UfoBlog.Pages.Components.Error TypeStr="1"></UfoBlog.Pages.Components.Error>
            </NotAuthorized>
        </AuthorizeRouteView>
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(MainLayout)">
            <UfoBlog.Pages.Components.Error TypeStr="2"></UfoBlog.Pages.Components.Error>
        </LayoutView>
    </NotFound>
</Router>

